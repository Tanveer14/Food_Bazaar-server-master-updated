      <Button fx:id="GoBackButton" layoutX="51.0" layoutY="42.0" mnemonicParsing="false" onAction="#GoBackButtonClicked" text="Go Back" />
      <ScrollPane layoutX="106.0" layoutY="96.0" prefHeight="388.0" prefWidth="612.0">
         <content>
            <Label fx:id="OrderLabel" alignment="TOP_LEFT">
               <font>
                  <Font size="15.0" />
               </font>
               <padding>
