<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="bgorderimage" stylesheets="@bgOrder.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrderViewController">
   <children>
      <Label alignment="CENTER" layoutX="273.0" layoutY="30.0" prefHeight="48.0" prefWidth="254.0" text="Orders ">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <Button fx:id="DoneButton" layoutX="620.0" layoutY="539.0" mnemonicParsing="false" onAction="#DoneButtonClicked" text="Done" />
      <Button fx:id="NextOrderButton" layoutX="680.0" layoutY="539.0" mnemonicParsing="false" onAction="#NextOrderButtonClicked" text="Next Order" />
